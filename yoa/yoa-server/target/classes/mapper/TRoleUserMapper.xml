<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yoa.mapper.TRoleUserMapper">

    <update id="updateRole">
        update
            t_role_user
        set
            roleId = #{roleid}
        where
              userId = #{userid}
    </update>
</mapper>
